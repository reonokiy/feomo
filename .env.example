# Instance Configuration
# Copy this file to .env.local and configure your instance

# Your GoToSocial instance URL (required)
# Example: https://social.example.com
VITE_GOTOSOCIAL_INSTANCE_URL=https://your-instance.social

# OAuth App Configuration
# These will be automatically registered if not provided
# You can also manually register an app and provide these values
VITE_GOTOSOCIAL_CLIENT_ID=
VITE_GOTOSOCIAL_CLIENT_SECRET=

# App Information (used for OAuth app registration)
VITE_APP_NAME=feomo
VITE_APP_WEBSITE=https://github.com/reonokiy/feomo
